{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "background": "#333238",
          "foreground": "#EFDCF9",
          "properties": {
            "alpine": "\uf300",
            "arch": "\uf303",
            "centos": "\uf304",
            "debian": "\uf306",
            "elementary": "\uf309",
            "fedora": "\uf30a",
            "gentoo": "\uf30d",
            "linux": "\ue712",
            "macos": "\uf179 ",
            "manjaro": "\uf312",
            "mint": "\uf30f",
            "opensuse": "\uf314",
            "raspbian": "\uf315",
            "ubuntu": "\uf31b ",
            "windows": "\ue62a ",
            "wsl": "\ue712"
          },
          "style": "powerline",
          "template": " {{ .Icon }}",
          "type": "os"
        },
        {
          "background": "#434248",
          "foreground": "#EFDCF9",
          "style": "powerline",
          "powerline_symbol": "\ue0c0",
          "template": "\ue683 {{ .Name }} ",
          "type": "shell"
        },
        {
          "background": "#535158",
          "foreground": "#EFDCF9",
          "style": "powerline",
          "powerline_symbol": "\ue0c0",
          "template": "\uf007 {{ .UserName }} ",
          "type": "session"
        },
        {
          "background": "#1068bf",
          "foreground": "#EFDCF9",
          "properties": {
            "folder_icon": "\ue5fe",
            "folder_separator_icon": " \u2771 ",
            "home_icon": "~",
            "max_depth": 2,
            "style": "agnoster_short"
          },
          "style": "powerline",
          "powerline_symbol": "\ue0c0",
          "template": "{{ .Path }} ",
          "type": "path"
        },
        {
          "background": "#ab6100",
          "foreground": "#EFDCF9",
          "properties": {
            "branch_template": "{{ trunc 25 .Branch }}",
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "style": "powerline",
          "powerline_symbol": "\ue0c0",
          "template": "{{ .HEAD }}{{ if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf01f{{ .Working.String }}{{ end }}{{ if .Staging.Changed }} \ue729{{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \uef96 {{ .StashCount }}{{ end }} ",
          "type": "git"
        },
        {
          "background": "#24663b",
          "foreground": "#EFDCF9",
          "properties": {
            "always_enabled": true
          },
          "style": "powerline",
          "powerline_symbol": "\ue0c0",
          "template": "{{ .FormattedMs }} ",
          "type": "executiontime"
        },
        {
          "background": "#108548",
          "foreground": "#EFDCF9",
          "background_templates": ["{{ if gt .Code 0 }}#ae1800{{ end }}"],
          "properties": {
            "always_enabled": true
          },
          "style": "diamond",
          "trailing_diamond": "\ue0c0",
          "template": "{{ if gt .Code 0 }}{{ .Code }} {{ end }}\uef9d ",
          "type": "status"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "background": "#626168",
          "foreground": "#EFDCF9",
          "style": "diamond",
          "trailing_diamond": "\ue0b0",
          "template": " \ue285",
          "type": "shell"
        }
      ],
      "type": "prompt"
    }
  ],
  "console_title": true,
  "console_title_style": "template",
  "console_title_template": "{{ .Folder }}",
  "transient_prompt": {
    "background": "transparent",
    "foreground": "#EFDCF9",
    "template": "\ue285 "
  },
  "final_space": true,
  "version": 3
}
